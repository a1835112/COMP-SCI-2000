/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=a,out=res1);
	Not(in=b,out=res2);
	And(a=a,b=res2,out=res3);
	And(a=res1,b=b,out=res4);
	Or(a=res3,b=res4,out=out);
}