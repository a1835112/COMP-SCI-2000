/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	 Not(in=sel,out=res1);
	 And(a=a,b=res1,out=res2);
	 And(a=sel,b=b,out=res3);
	 Or(a=res2,b=res3,out=out);
}